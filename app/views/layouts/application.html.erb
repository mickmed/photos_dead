<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
<%= stylesheet_link_tag 'application', media: 'all',
                                           'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<!-- <link rel="stylesheet" href="/assets/stylesheets/superslides.css">  -->
	<!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/jquery.superslides/0.6.2/stylesheets/superslides.css"> -->
 <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <!-- <script src="/assets/javascripts/jquery.easing.1.3.js"></script> -->
 <!-- <script src="/assets/javascripts/jquery.animate-enhanced.min.js"></script>  -->
  <!-- <script src="/assets/javascripts/jquery.superslides.js" type="text/javascript" charset="utf-8"></script> -->
  <script src="https://cdn.jsdelivr.net/jquery.superslides/0.6.2/jquery.superslides.js"></script>
</head>
 <body>
  <%= render 'layouts/header' %>




<% if @r %>
  <div id="slides" style="display:none;">	 
   <div class = "height-fill">
    <div class="slides-container">
    <% @r.each do |photo| %>
   	<%= photo.title %>
  	<%= link_to image_tag(photo.picture), {action: 'show', controller: 'photos', id: photo.id} %>
  <% end %>
    </div>
  </div>
 <div id="fullNav">
  <nav class="slides-navigation">
  	
   <a href="#" class="next">Next</a>
   <a href="#" class="prev">Previous</a>
  </nav>
  </div>
 </div>
 <% end %>


  
  
    <div class="container">
      <%= yield %> 
      <%= render 'layouts/footer' %>

    </div>

  </body>
</html>